    "rollup-plugin-uglify": "^6.0.4",
    "sass": "^1.26.10",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "vite": "1.0.0-beta.11",
    "vue": "3.0.0-rc.2",
    "vue-eslint-parser": "^7.1.0",
    "vue-router": "4.0.0-beta.2",
    "vuex": "4.0.0-beta.4"
  }
