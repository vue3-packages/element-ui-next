{
  "name": "element-ui-next",
  "description": "A Component Library for Vue3.",
  "main": "dist/element-ui-next.umd.min.js",
  "module": "dist/element-ui-next.esm.js",
  "version": "0.0.1",
  "repository": {
    "type": "url",
    "url": "https://github.com/vue3-packages/element-ui-next.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue3-packages/element-ui-next/issues"
  },
  "unpkg": "dist/element-ui-next.umd.js",
  "style": "dist/style/element-ui-next.min.css",
  "keywords": [
    "element-ui",
    "vue-next",
    "vue3"
  ],
  "files": [
    "dist",
    "src",
    "packages"
  ],
  "scripts": {
    "examples": "vite",
    "build:esm": "rollup -c --environment FORMAT:esm",
    "build:umd": "rollup -c --environment FORMAT:umd",
    "build:min": "rollup -c --environment FORMAT:min",
    "build:css": "rimraf dist/style/* && rollup --config ./scripts/rollup.css.js",
    "build": "rimraf dist/* && run-p build:esm build:umd build:min build:css",
    "lint": "eslint 'src/**/*.{js,ts}'"
  },
  "devDependencies": {
    "@ant-design-vue/babel-plugin-jsx": "1.0.0-beta.4",
    "@babel/core": "^7.10.5",
