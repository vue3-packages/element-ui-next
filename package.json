    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-alpha.10",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash.merge": "^4.6.2",
    "markdown-it": "^11.0.0",
    "markdown-it-container": "^3.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
